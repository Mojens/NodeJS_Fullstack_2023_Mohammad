{
  "routes": [
    {
      "src": "/frontpage/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "text/css"
      },
      "dest": "/frontpage/$1"
    },
    {
      "src": "/frontpage/(.*)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Content-Type": "application/javascript"
      },
      "dest": "/frontpage/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
